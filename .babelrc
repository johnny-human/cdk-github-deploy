{
    "presets": [
      ["@babel/preset-env", {
        "targets": {
          "node": "current"
        },
        "useBuiltIns": "usage",
        "corejs": 3
      }],
      "@babel/preset-typescript"
    ],
    "plugins": [
      ["transform-imports", {
        "@vercel/node": {
          "transform": "@vercel/node/${member}",
          "preventFullImport": true
        }
      }],
      ["@babel/plugin-transform-runtime", {
        "corejs": 3,
        "useESModules": true
      }]
    ]
  }